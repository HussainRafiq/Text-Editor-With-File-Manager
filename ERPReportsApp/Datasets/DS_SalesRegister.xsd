            <MainSource>
              <DbSource ConnectionRef="ConnectionString (Web.config)" DbObjectName="SELECT si.InvoiceDate AS [Date], sid.InvoiceID, si.SalesOrderID AS BT, c.FullName, c.NTN, ? AS FromDate, ? AS ToDate, ISNULL(SUM(ISNULL(sid.Qty, 0.00) * ISNULL(sid.Rate, 0.00)), 0.00) AS Amount, sid.DiscountPercentage, ISNULL(SUM(sid.Qty), 0.00) AS DeliveredQty, ISNULL((SELECT ISNULL(TaxPercentage,0.00) FROM SalesInvoiceTax WHERE InvoiceID = si.ID AND CompanyCode = si.CompanyCode AND IsDelete = 0 AND [Status] = 'Active'),0.00) AS TaxPercentage FROM SalesInvoiceDetail AS sid INNER JOIN SalesInvoice AS si ON si.ID = sid.InvoiceID AND si.CompanyCode = sid.CompanyCode AND si.[Status] = 'Active' INNER JOIN Customer AS c ON c.ID = si.CustomerID AND c.CompanyCode = si.CompanyCode AND c.[Status] = 'Active' WHERE (si.InvoiceDate &gt;= CAST(CONVERT(date, ?) AS nvarchar) + ' 00:00:00.000') AND (si.InvoiceDate &lt;= CAST(CONVERT(date, ?) AS nvarchar) + ' 23:59:59.996') AND (sid.CompanyCode = ?) AND (sid.IsDelete = 0) AND sid.[Status] = 'Active' GROUP BY sid.InvoiceID, sid.DiscountPercentage, si.SalesOrderID, si.InvoiceDate, c.FullName, c.NTN, sid.CompanyCode,si.ID,si.CompanyCode" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
SELECT *, Amount - (Amount * DiscountPercentage/100) + (Amount * TaxPercentage/100) as AmountIncTax FROM 
(
SELECT si.InvoiceDate AS [Date], sid.InvoiceID, si.SalesOrderID AS BT, c.FullName, c.NTN, ? AS FromDate, ? AS ToDate, ISNULL(SUM(ISNULL(sid.Qty, 0.00) * ISNULL(sid.Rate, 0.00)), 0.00) AS Amount, sid.DiscountPercentage, 
                  ISNULL(SUM(sid.Qty), 0.00) AS DeliveredQty,
       ISNULL((SELECT ISNULL(TaxPercentage,0.00) From SalesInvoiceTax WHERE InvoiceID = si.ID AND CompanyCode = si.CompanyCode AND IsDelete = 0 AND [Status] = 'Active'),0.00) as TaxPercentage
	   
	   
FROM     SalesInvoiceDetail AS sid 
INNER JOIN SalesInvoice AS si ON si.ID = sid.InvoiceID AND si.CompanyCode = sid.CompanyCode  AND si.[Status] = 'Active'
INNER JOIN Customer AS c ON c.ID = si.CustomerID AND c.CompanyCode = si.CompanyCode AND c.[Status] = 'Active'
WHERE (si.InvoiceDate &gt;= CAST(CONVERT(date, ?) AS nvarchar) + ' 00:00:00.000') AND (si.InvoiceDate &lt;= CAST(CONVERT(date, ?) AS nvarchar) + ' 23:59:59.996') AND (sid.CompanyCode = ?) AND (sid.IsDelete = 0) AND sid.[Status] = 'Active'
GROUP BY sid.InvoiceID, sid.DiscountPercentage, si.SalesOrderID, si.InvoiceDate, c.FullName, c.NTN, sid.CompanyCode,si.ID,si.CompanyCode
) AS dt1
ORDER BY [Date]
</CommandText>
