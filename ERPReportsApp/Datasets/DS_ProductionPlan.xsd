                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT pp.ProductionPlanDate, ppd.ProductionPlanID, ppd.ID as ProductionPlanDetailID, ppd.[Status], drcd.ID as DyeingReceiptChallanDetailID,
		drcd.DyeingReceiptChallanID, drcd.DyeingChallanDetailID, ppd.Bundle as DRCBundles, drcd.[Sequence], wod.NetWeight, wod.GrossWeight,
		sod.Color, sod.CountID, sod.VolumeID, sod.SalesOrderID, sod.Bundle SOBundles,
		CONCAT(ISNULL(so.PONumber, '-'), '/', ISNULL(so.SalesContractNumber, '-'), '/', ISNULL(so.JobNumber, '-')) as Programme,
		so.CustomerID, cust.FullName as CustomerName, co.Title as CountName, vol.Title as VolumeAmt, vol.[Value] as VolumeUOM,
		ppd.SpoolingEmployeeID, spe.FullName as SpoolingEmpName, ppd.WindingEmployeeID, wie.FullName as WindingEmpName
FROM [ProductionPlanDetail] ppd
JOIN [ProductionPlan] pp ON ppd.ProductionPlanID = pp.ID AND ppd.CompanyCode = pp.CompanyCode AND pp.IsDelete = 0
JOIN [DyeingReceiptChallanDetail] drcd ON ppd.DyeingReceiptChallanDetailID = drcd.ID AND ppd.CompanyCode = drcd.CompanyCode AND drcd.IsDelete = 0
JOIN [WorkOrderDetail] wod ON drcd.DyeingChallanDetailID = wod.ID AND drcd.CompanyCode = wod.CompanyCode AND wod.IsDelete = 0
JOIN [SalesOrderDetail] sod ON wod.SalesOrderDetailID = sod.ID AND wod.CompanyCode = sod.CompanyCode AND sod.IsDelete = 0
JOIN [SalesOrder] so ON sod.SalesOrderID = so.ID AND sod.CompanyCode = so.CompanyCode AND so.IsDelete = 0
JOIN [Customer] cust ON so.CustomerID = cust.ID AND so.CompanyCode = cust.CompanyCode AND cust.IsDelete = 0
JOIN [Count] co ON sod.CountID = co.ID AND sod.CompanyCode = co.CompanyCode AND co.IsDelete = 0
JOIN [Volume] vol ON sod.VolumeID = vol.ID AND sod.CompanyCode = vol.CompanyCode AND vol.IsDelete = 0
JOIN [Employee] spe ON ppd.SpoolingEmployeeID = spe.ID AND ppd.CompanyCode = ppd.CompanyCode AND spe.IsDelete = 0
JOIN [Employee] wie ON ppd.WindingEmployeeID = wie.ID AND ppd.CompanyCode = wie.CompanyCode AND wie.IsDelete = 0
WHERE ppd.ProductionPlanID = ? AND ppd.[CompanyCode] = ? AND ppd.IsDelete = 0</CommandText>
