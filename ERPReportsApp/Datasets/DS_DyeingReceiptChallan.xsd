                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT drcd.ID as DyeingReceiptChallanDetailID, drcd.DyeingReceiptChallanID, drc.DyeingUnitID, drc.ReferenceNo, wod.ID as WorkOrderDetailID, wod.WorkOrderID, wod.SalesOrderDetailID, drcd.[Sequence],
			wod.Bundle as BundlesSent, wod.NetWeight, wod.GrossWeight, drcd.Quality, drcd.Bundle as RemainingBundle, sod.Color, drc.IssueDate,
			sod.[Standard], sod.CountID, c.Title as CountName, wo.SalesOrderID, so.CustomerID, so.PONumber, so.SalesContractNumber, so.JobNumber, cust.FullName as CustomerName, u.FullName as DyeingUnitName
FROM [DyeingReceiptChallanDetail] drcd
JOIN [DyeingReceiptChallan] drc ON drcd.DyeingReceiptChallanID = drc.ID AND drcd.[CompanyCode] = drc.[CompanyCode] AND drc.[IsDelete] = 0
JOIN [WorkOrderDetail] wod ON drcd.DyeingChallanDetailID = wod.ID AND drcd.CompanyCode = wod.CompanyCode AND wod.IsDelete = 0
JOIN [WorkOrder] wo ON wod.WorkOrderID = wo.ID AND wod.CompanyCode = wo.CompanyCode AND wod.IsDelete = 0
JOIN [SalesOrderDetail] sod ON wod.SalesOrderDetailID = sod.ID AND wod.CompanyCode = sod.CompanyCode AND sod.IsDelete = 0
JOIN [Count] c ON sod.CountID = c.ID AND sod.CompanyCode = c.CompanyCode AND c.IsDelete = 0
JOIN [SalesOrder] so ON wo.SalesOrderID = so.ID AND wo.CompanyCode = so.CompanyCode AND so.IsDelete = 0
JOIN [Customer] cust ON so.CustomerID = cust.ID AND so.CompanyCode = cust.CompanyCode AND cust.IsDelete = 0
JOIN [Unit] u ON drc.DyeingUnitID = u.ID AND drc.CompanyCode = u.CompanyCode AND u.IsDelete = 0
WHERE drcd.[DyeingReceiptChallanID] = ? AND drcd.CompanyCode = ? AND drcd.IsDelete = 0</CommandText>
