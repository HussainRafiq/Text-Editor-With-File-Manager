                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT dcd.DeliveryChallanID, dc.DeliveryChallanDate, dc.DeliveryMan, dcd.ID as DeliveryChallanDetailID, sod.ID as SalesOrderDetailID, '0' as ProductionPlanDetailID, sod.Color, sod.CountID, sod.VolumeID, sod.SalesOrderID,
		sod.Bundle as SOBundles, sod.Quantity as SOQuantity, dcd.NetWeight, dcd.GrossWeight, sod.[Standard], dcd.DeliveryQuantity as RemainingQty, CONCAT(ISNULL(so.PONumber, '-'), '/', ISNULL(so.SalesContractNumber, '-'), '/', ISNULL(so.JobNumber, '-')) as Programme,
		so.CustomerID, co.Title as CountName, vol.Title as VolumeAmt, vol.[Value] as VolumeUOM, cus.FullName as CustomerName
FROM [DeliveryChallanDetail] dcd
JOIN [DeliveryChallan] dc ON dcd.DeliveryChallanID = dc.ID AND dcd.[CompanyCode] = dc.CompanyCode AND dc.IsDelete = 0
JOIN [Customer] cus ON dc.CustomerID = cus.ID AND dc.CompanyCode = cus.CompanyCode AND cus.IsDelete = 0
JOIN [SalesOrderDetail] sod ON dcd.SalesOrderDetailID = sod.ID AND dcd.CompanyCode = sod.CompanyCode AND sod.IsDelete = 0
JOIN [SalesOrder] so ON sod.SalesOrderID = so.ID AND sod.CompanyCode = so.CompanyCode AND so.IsDelete = 0
JOIN [Count] co ON sod.CountID = co.ID AND sod.CompanyCode = co.CompanyCode AND co.IsDelete = 0
JOIN [Volume] vol ON sod.VolumeID = vol.ID AND sod.CompanyCode = vol.CompanyCode AND vol.IsDelete = 0
WHERE dcd.[DeliveryChallanID] = ? AND dcd.CompanyCode = ? AND dcd.IsDelete = 0</CommandText>
