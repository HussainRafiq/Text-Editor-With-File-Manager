                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        sod.Bundle, sod.NetWeight, sod.GrossWeight, sod.Color, sod.CountID, co.Title AS CountName, sod.Quantity, sod.VolumeID, CONCAT(vol.Title, ' (', vol.Value, ')') AS VolumeName, sod.Standard, sod.Quality, sod.DyeingUnitID, u.FullName AS DyeingUnitName, so.ID as SalesOrderID,
                         so.CustomerID, cus.FullName AS CustomerName, so.PONumber, so.SalesContractNumber, so.JobNumber, so.IssueDate
FROM            SalesOrderDetail AS sod INNER JOIN
                         SalesOrder AS so ON sod.SalesOrderID = so.ID AND sod.CompanyCode = so.CompanyCode AND so.IsDelete = 0 INNER JOIN
                         Customer AS cus ON so.CustomerID = cus.ID AND so.CompanyCode = cus.CompanyCode AND cus.IsDelete = 0 INNER JOIN
                         Count AS co ON sod.CountID = co.ID AND sod.CompanyCode = co.CompanyCode AND co.IsDelete = 0 INNER JOIN
                         Volume AS vol ON sod.VolumeID = vol.ID AND sod.CompanyCode = vol.CompanyCode AND vol.IsDelete = 0 INNER JOIN
                         Unit AS u ON sod.DyeingUnitID = u.ID AND sod.CompanyCode = u.CompanyCode AND u.IsDelete = 0
WHERE        (sod.SalesOrderID = ?) AND (sod.CompanyCode = ?) AND (sod.IsDelete = 0)</CommandText>
